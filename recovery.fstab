/efs            emmc        /dev/block/platform/dw_mmc.0/by-name/EFS          flags=backup=1;display=EFS
/boot           emmc        /dev/block/platform/dw_mmc.0/by-name/BOOT
/system         ext4        /dev/block/platform/dw_mmc.0/by-name/SYSTEM
/data           ext4        /dev/block/platform/dw_mmc.0/by-name/USERDATA     length=-16384
/cache          ext4        /dev/block/platform/dw_mmc.0/by-name/CACHE
/recovery       emmc        /dev/block/platform/dw_mmc.0/by-name/RECOVERY     flags=backup=1;display=Recovery
/modem          emmc        /dev/block/platform/dw_mmc.0/by-name/RADIO        flags=backup=1;display=Modem

/external_sd    vfat        /dev/block/mmcblk1p1 /dev/block/mmcblk1           flags=removable;storage;display="Micro SD"
/usb-otg        vfat        /dev/block/sda1      /dev/block/sda               flags=removable;storage;display=USB-OTG